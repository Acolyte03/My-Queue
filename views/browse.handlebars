<h1>Browse TV Shows</h1>
     <p>Note: you are viewing browse.handlebars</p>

{{#if tvshows.length}}
  <div class="tvshow-list">
    {{#each tvshows}}
      <div class="tvshow">
        <h2>{{this.name}}</h2>
        <!-- Add more properties as needed -->
        <p>{{this.overview}}</p>
      </div>
    {{/each}}
  </div>
{{else}}
  <p>No TV shows found.</p>
{{/if}}
